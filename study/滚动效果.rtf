<script>\par
var ScrollWin = \{\par
w3c : document.getElementById,\par
iex : document.all,\par
scrollLoop : false,\par
scrollInterval : null, // setInterval id\par
currentBlock : null,   // object reference\par
getWindowHeight : function()\{\par
if(this.iex) return (document.documentElement.clientHeight) ?\par
document.documentElement.clientHeight : document.body.clientHeight;\par
else return window.innerHeight;\par
\},\par
getScrollLeft : function()\{\par
if(this.iex) return (document.documentElement.scrollLeft) ?\par
document.documentElement.scrollLeft : document.body.scrollLeft;\par
else return window.pageXOffset;\par
\},\par
getScrollTop : function()\{\par
if(this.iex) return (document.documentElement.scrollTop) ?\par
document.documentElement.scrollTop : document.body.scrollTop;\par
else return window.pageYOffset;\par
\},\par
getElementYpos : function(el)\{\par
var y = 0;\par
while(el.offsetParent)\{\par
y += el.offsetTop\par
el = el.offsetParent;\par
\}//\'bb\'b6\'d3\'ad\'c0\'b4\'b5\'bd\'d5\'be\'b3\'a4\'cc\'d8\'d0\'a7\'cd\'f8\'a3\'ac\'ce\'d2\'c3\'c7\'b5\'c4\'cd\'f8\'d6\'b7\'ca\'c7{\field{\*\fldinst{HYPERLINK "www.zzjs.net"}}{\fldrslt{\ul\cf1 www.zzjs.net}}}\f0\fs22\'a3\'ac\'ba\'dc\'ba\'c3\'bc\'c7\'a3\'aczz\'d5\'be\'b3\'a4\'a3\'acjs\'be\'cd\'ca\'c7js\'cc\'d8\'d0\'a7\'a3\'ac\'b1\'be\'d5\'be\'ca\'d5\'bc\'af\'b4\'f3\'c1\'bf\'b8\'df\'d6\'ca\'c1\'bfjs\'b4\'fa\'c2\'eb\'a3\'ac\'bb\'b9\'d3\'d0\'d0\'ed\'b6\'e0\'b9\'e3\'b8\'e6\'b4\'fa\'c2\'eb\'cf\'c2\'d4\'d8\'a1\'a3\par
return y;\par
\},\par
scroll : function(num)\{\par
if(!this.w3c)\{\par
location.href = "#"+this.anchorName+num;\par
return;\par
\}\par
if(this.scrollLoop)\{\par
clearInterval(this.scrollInterval);\par
this.scrollLoop = false;\par
this.scrollInterval = null;\par
\}//\'bb\'b6\'d3\'ad\'c0\'b4\'b5\'bd\'d5\'be\'b3\'a4\'cc\'d8\'d0\'a7\'cd\'f8\'a3\'ac\'ce\'d2\'c3\'c7\'b5\'c4\'cd\'f8\'d6\'b7\'ca\'c7{\field{\*\fldinst{HYPERLINK "www.zzjs.net"}}{\fldrslt{\ul\cf1 www.zzjs.net}}}\f0\fs22\'a3\'ac\'ba\'dc\'ba\'c3\'bc\'c7\'a3\'aczz\'d5\'be\'b3\'a4\'a3\'acjs\'be\'cd\'ca\'c7js\'cc\'d8\'d0\'a7\'a3\'ac\'b1\'be\'d5\'be\'ca\'d5\'bc\'af\'b4\'f3\'c1\'bf\'b8\'df\'d6\'ca\'c1\'bfjs\'b4\'fa\'c2\'eb\'a3\'ac\'bb\'b9\'d3\'d0\'d0\'ed\'b6\'e0\'b9\'e3\'b8\'e6\'b4\'fa\'c2\'eb\'cf\'c2\'d4\'d8\'a1\'a3\par
if(this.currentBlock != null) this.currentBlock.className = this.offClassName;\par
this.currentBlock = document.getElementById(this.blockName+num);\par
this.currentBlock.className = this.onClassName;\par
var doc = document.getElementById(this.containerName);\par
var documentHeight = this.getElementYpos(doc) + doc.offsetHeight;\par
var windowHeight = this.getWindowHeight();\par
var ypos = this.getElementYpos(this.currentBlock);\par
if(ypos > documentHeight - windowHeight) ypos = documentHeight - windowHeight;\par
this.scrollTo(0,ypos);\par
\},\par
scrollTo : function(x,y)\{\par
if(this.scrollLoop)\{\par
var left = this.getScrollLeft();\par
var top = this.getScrollTop();\par
if(Math.abs(left-x) <= 1 && Math.abs(top-y) <= 1)\{\par
window.scrollTo(x,y);\par
clearInterval(this.scrollInterval);\par
this.scrollLoop = false;\par
this.scrollInterval = null;\par
\}else\{\par
window.scrollTo(left+(x-left)/10, top+(y-top)/10);\par
\}//\'bb\'b6\'d3\'ad\'c0\'b4\'b5\'bd\'d5\'be\'b3\'a4\'cc\'d8\'d0\'a7\'cd\'f8\'a3\'ac\'ce\'d2\'c3\'c7\'b5\'c4\'cd\'f8\'d6\'b7\'ca\'c7{\field{\*\fldinst{HYPERLINK "www.zzjs.net"}}{\fldrslt{\ul\cf1 www.zzjs.net}}}\f0\fs22\'a3\'ac\'ba\'dc\'ba\'c3\'bc\'c7\'a3\'aczz\'d5\'be\'b3\'a4\'a3\'acjs\'be\'cd\'ca\'c7js\'cc\'d8\'d0\'a7\'a3\'ac\'b1\'be\'d5\'be\'ca\'d5\'bc\'af\'b4\'f3\'c1\'bf\'b8\'df\'d6\'ca\'c1\'bfjs\'b4\'fa\'c2\'eb\'a3\'ac\'bb\'b9\'d3\'d0\'d0\'ed\'b6\'e0\'b9\'e3\'b8\'e6\'b4\'fa\'c2\'eb\'cf\'c2\'d4\'d8\'a1\'a3\par
\}else\{\par
this.scrollInterval = setInterval("ScrollWin.scrollTo("+x+","+y+")",20);\par
this.scrollLoop = true;\par
\}\par
\}//\'bb\'b6\'d3\'ad\'c0\'b4\'b5\'bd\'d5\'be\'b3\'a4\'cc\'d8\'d0\'a7\'cd\'f8\'a3\'ac\'ce\'d2\'c3\'c7\'b5\'c4\'cd\'f8\'d6\'b7\'ca\'c7{\field{\*\fldinst{HYPERLINK "www.zzjs.net"}}{\fldrslt{\ul\cf1 www.zzjs.net}}}\f0\fs22\'a3\'ac\'ba\'dc\'ba\'c3\'bc\'c7\'a3\'aczz\'d5\'be\'b3\'a4\'a3\'acjs\'be\'cd\'ca\'c7js\'cc\'d8\'d0\'a7\'a3\'ac\'b1\'be\'d5\'be\'ca\'d5\'bc\'af\'b4\'f3\'c1\'bf\'b8\'df\'d6\'ca\'c1\'bfjs\'b4\'fa\'c2\'eb\'a3\'ac\'bb\'b9\'d3\'d0\'d0\'ed\'b6\'e0\'b9\'e3\'b8\'e6\'b4\'fa\'c2\'eb\'cf\'c2\'d4\'d8\'a1\'a3\par
\};\par
ScrollWin.containerName = "container";\par
ScrollWin.anchorName    = "anchor";\par
ScrollWin.blockName     = "block";\par
ScrollWin.onClassName   = "active";\par
ScrollWin.offClassName  = "visited";\par
</script>\par
